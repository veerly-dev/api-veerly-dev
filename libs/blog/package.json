{
  "name": "@veerly-dev/blog",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@veerly-dev/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/libs/blog",
          "tsConfig": "libs/blog/tsconfig.lib.json",
          "packageJson": "libs/blog/package.json",
          "main": "libs/blog/src/index.ts",
          "assets": [
            "libs/blog/*.md"
          ]
        }
      }
    }
  }
}
