{
  "name": "@linkstack-hub/linkstack-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@linkstack-hub/linkstack"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/linkstack-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@linkstack-hub/linkstack:build",
          "@linkstack-hub/linkstack:serve"
        ]
      }
    }
  }
}
