{
  "name": "@veerly-dev/source",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prisma:stitch": "ts-node apps/linkstack/prisma/stitch-prisma-schema.ts",
    "prisma:build": "prisma generate --schema=apps/linkstack/prisma/schema.prisma  && nx build @veerly-dev/linkstack",
    "prisma:migrate": "cd apps/linkstack/prisma && npx prisma migrate dev",
    "prisma:stitch-migrate": "ts-node apps/linkstack/prisma/stitch-prisma-schema.ts && cd apps/linkstack/prisma && npx prisma migrate dev",
    "prisma:seed": "cd apps/linkstack/prisma && npx prisma db seed",
    "dev": "nx serve @veerly-dev/linkstack",
    "start": "node dist/apps/linkstack/main.js",
    "build": "nx run-many --target=build --projects=@veerly-dev/prisma,@veerly-dev/auth-google,@veerly-dev/organization,@veerly-dev/linkstack",
    "build:all": "nx run-many --target=build --all"
  },
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/eslint": "21.5.3",
    "@nx/eslint-plugin": "21.5.3",
    "@nx/jest": "21.5.3",
    "@nx/js": "21.5.3",
    "@nx/node": "21.5.3",
    "@nx/web": "21.5.3",
    "@nx/webpack": "21.5.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.38",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "21.5.3",
    "prettier": "^2.6.2",
    "prisma": "^6.16.1",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "packages/*",
    "apps/*",
    "libs/*"
  ],
